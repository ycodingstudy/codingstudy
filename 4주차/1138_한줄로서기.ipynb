   "source": [
    "n = int(input())\n",
    "s = list(map(int, input().split()))\n",
    "a = [n]\n",
    "for i in range(n-1, 0, -1):\n",
    "    no = s[i-1]\n",
    "    \n",
    "    if no == 0:\n",
    "        a = [i] + a\n",
    "    else:\n",
    "        a = a[:no] + [i] + a[no:]\n",
    "for i in a:\n",
    "    print(i, end=' ')"
   ]
  }
